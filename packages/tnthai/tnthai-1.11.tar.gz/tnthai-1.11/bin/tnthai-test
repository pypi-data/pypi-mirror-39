#!/usr/bin/env python 
#  -*- coding: UTF-8 -*-

import os
import sys

if (sys.version_info > (3, 0)):
    for p in sys.path:
        if "site-packages" in p:
            path = p + "/tnthai/swathclone/tests/test_tnthai_py3.py"
    os.system("python3 " + path)
else:
    for p in sys.path:
        if "site-packages" in p:
            path = p + "/tnthai/swathclone/tests/test_tnthai_py2.py"
    os.system("python " + path)

