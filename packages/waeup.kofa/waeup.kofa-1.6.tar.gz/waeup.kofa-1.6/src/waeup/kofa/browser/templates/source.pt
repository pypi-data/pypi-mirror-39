<div class="panel panel-default">
  <div class="panel-heading" role="tab"
       tal:attributes="id viewlet/heading">
    <h4 class="panel-title">
      <a class="collapsed" data-toggle="collapse"
         tal:attributes="data-parent viewlet/accordion;
                         href viewlet/hash_collapse;
                         aria-controls viewlet/collapse"
         aria-expanded="false">
        <span tal:replace="viewlet/title">TITLE</span>
      </a>
    </h4>
  </div>
  <div tal:attributes="id viewlet/collapse;
                       aria-labelledby viewlet/heading"
       class="panel-collapse collapse"
       role="tabpanel">
    <div class="panel-body">
      <table class="table">
        <thead>
          <tr>
            <th i18n:translate="">Value</th>
            <th i18n:translate="">Title</th>
          </tr>
        </thead>
        <tbody>
          <tr tal:repeat="item viewlet/source">
            <td tal:content="python: item[0]">NAME</td>
            <td tal:content="python: item[1]">TITLE</td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>