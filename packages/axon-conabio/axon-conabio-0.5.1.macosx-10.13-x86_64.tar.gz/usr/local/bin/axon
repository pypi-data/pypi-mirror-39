#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'axon-conabio==0.5.1','console_scripts','axon'
__requires__ = 'axon-conabio==0.5.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('axon-conabio==0.5.1', 'console_scripts', 'axon')()
    )
